void insertionSort(int *input, int size)
{
    //Write your code here
    int i, j, temp;  
    for (i = 1; i < size; i++) {  
        temp = input[i];  
        j = i - 1;  
  
        while(j>=0 && temp <= input[j])  
        {    
            input[j+1] = input[j];     
            j = j-1;    
        }    
        input[j+1] = temp;    
    }  

}
